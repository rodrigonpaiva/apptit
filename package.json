{
  "name": "@apptit/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "typecheck": "nx run-many --target=typecheck",
    "lint:all": "nx run-many --target=lint",
    "format": "nx format:write",
    "e2e": "nx run-many --target=e2e",
    "dev:api": "nx run api:serve",
    "dev:web": "nx run web:dev",
    "dev:mobile": "nx run mobile:start",
    "build:all": "nx affected -t build",
    "test:all": "nx affected -t test",
    "gql:codegen": "graphql-codegen --config packages/graphql/codegen.ts",
    "gql:watch": "graphql-codegen --watch --config packages/graphql/codegen.ts",
    "gf:start:feature": "node -e \"console.log('Use: git checkout -b feature/$npm_config_name develop')\"",
    "gf:finish:feature": "node -e \"console.log('Open PR to develop and squash-merge')\"",
    "gf:start:release": "node -e \"console.log('Use: git checkout -b release/$npm_config_version develop && npx changeset version')\"",
    "gf:finish:release": "node -e \"console.log('Merge release/* -> main, tag v$npm_config_version, then merge back to develop')\"",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "private": true,
