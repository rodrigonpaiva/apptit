{
  "name": "@apptit/web",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "web",
    "tags": [
      "scope:web",
      "type:app"
    ],
    "targets": {
      "dev": {
        "executor": "nx:run-commands",
        "options": {
          "command": "next dev",
          "cwd": "apps/web"
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "next build",
          "cwd": "apps/web"
        }
      },
      "start": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build"
        ],
        "options": {
          "command": "next start",
          "cwd": "apps/web"
        }
      },
      "serve-static": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build"
        ],
        "options": {
          "command": "next start",
          "cwd": "apps/web"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ],
        "options": {
          "lintFilePatterns": [
            "apps/web/**/*.ts",
            "apps/web/**/*.tsx"
          ]
        }
      },
      "typecheck": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npx tsc --project apps/web/tsconfig.json --noEmit"
        }
      }
    }
  },
  "dependencies": {
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }
}
