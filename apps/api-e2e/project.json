{
  "name": "api-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api-e2e/src",
  "projectType": "library",
  "tags": [
    "scope:api-e2e",
    "type:e2e"
  ],
  "// targets": "to see all targets run: nx show project api-e2e --web",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "tsConfig": "apps/api-e2e/tsconfig.lib.json",
        "main": "apps/api-e2e/src/index.ts",
        "outputPath": "dist/apps/api-e2e"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/api-e2e/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{projectRoot}/test-output/jest"
      ],
      "options": {
        "jestConfig": "apps/api-e2e/jest.config.ts"
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx test api-e2e"
      },
      "dependsOn": [
        "api:build"
      ]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsc --project apps/api-e2e/tsconfig.lib.json --noEmit"
      }
    }
  },
  "implicitDependencies": [
    "api"
  ]
}
