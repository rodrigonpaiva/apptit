{
  "name": "web-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "web"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/playwright:playwright",
        "options": {
          "config": "apps/web-e2e/playwright.config.ts"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ],
        "options": {
          "lintFilePatterns": [
            "apps/web-e2e/**/*.ts"
          ]
        }
      },
      "typecheck": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npx tsc --project apps/web-e2e/tsconfig.json --noEmit"
        }
      }
    }
  }
}
