{
  "name": "@apptit/gateway-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@apptit/gateway",
      "@apptit/auth"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "e2e/gateway-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@apptit/gateway:build",
          "@apptit/gateway:serve",
          "@apptit/auth:build",
          "@apptit/auth:serve"
        ]
      }
    }
  }
}
